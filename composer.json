{
    "name": "ottosmops/settings",
    "description": "A simple settings package for laravel",
    "type": "library",
    "keywords": [
        "anton",
        "ottosmops",
        "laravel",
        "settings"
    ],
    "authors": [
        {
            "name": "ak",
            "email": "kraenzle@k-r.ch",
            "homepage": "https://wwwk-r.ch"
        }
    ],
    "homepage": "https://github.com/ottosmops/settings",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^6.0.26|7.*|8.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0"
    },
     "autoload": {
        "psr-4": {
            "Ottosmops\\Settings\\": "src/"
        },
        "files": [
            "src/Utils/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ottosmops\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ottosmops\\Settings\\SettingsServiceProvider"
            ]
        }
    }
}
